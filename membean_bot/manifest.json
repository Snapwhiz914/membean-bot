{
    "name": "Membean Botter",
    "description": "Don't do membean",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "host_permissions": [
        "https://membean.com/"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}